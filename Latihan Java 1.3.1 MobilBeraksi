package textbasedapplication;

public class MobilBeraksi {
    public static void main(String[] args) {
        Mobil mobilAditya = new Mobil();
        
        mobilAditya.warna = "Stripes";
        mobilAditya.tahunProduksi = 2019;
        
        System.out.println("Warna : " + mobilAditya.warna);
        System.out.println("Tahun Produksi : " + mobilAditya.tahunProduksi );
    }
}
